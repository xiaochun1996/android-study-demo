<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jock.main">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.jock.library.base.BaseApplication"
        android:theme="@style/Theme.Timer">
        <activity android:name="com.jock.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jock.main.SecondActivity" />
        <activity android:name="com.jock.glide.GlideActivity" />
        <activity android:name="com.jock.route.activity.LoginActivity" />
        <activity android:name="com.jock.route.activity.InformActivity" />
        <activity android:name="com.jock.route.activity.RouteHomeActivity" />
        <activity android:name="com.jock.route.activity.DegradeGlobalActivity" />
        <activity android:name="com.jock.util.ActivityManagerActivity" />
        <activity android:name="com.jock.thread.ThreadTest2Activity" />
        <activity android:name="com.jock.thread.ThreadTest3Activity" />
        <activity android:name="com.jock.thread.ThreadTest4Activity" />
        <activity android:name="com.jock.thread.ThreadTest5Activity" />
        <activity android:name="com.jock.thread.ThreadTest12Activity" />
        <activity android:name="com.jock.thread.CoroutinesDemoActivity2" />
    </application>

</manifest>